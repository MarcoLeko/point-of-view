    "template",
    "view",
    "speed",
    "handlebars"
  ],
  "author": "Marco Leko Industries",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarcoLeko/point-of-view/issues"
  },
  "homepage": "https://github.com/MarcoLeko/point-of-view#readme",
  "dependencies": {
    "handlebars": "^4.7.7",
    "promised-handlebars": "^2.0.1",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.0",
    "art-template": "^4.13.2",
    "cross-env": "^7.0.2",
    "dot": "^1.1.3",
    "ejs": "^3.1.2",
    "eta": "^1.11.0",
    "express": "^4.17.1",
    "fastify": "^3.0.0",
    "html-minifier": "^4.0.0",
    "html-minify-stream": "^2.0.0",
    "pino": "^6.7.0",
